{
    "version": 2,
    "builds": [
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/index.html", "dest": "/index.html" },
        { "src": "/index.js", "dest": "/index.js" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
